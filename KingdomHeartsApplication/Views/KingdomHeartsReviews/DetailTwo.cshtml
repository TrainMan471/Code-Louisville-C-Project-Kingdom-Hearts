@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <link rel="stylesheet" href="~/Styles/prettify.css" />
    <title></title>
</head>
<body>
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            <!-- Add menu items here -->
            <li><a href="@Url.Action("Index","KingdomHeartsReviews")">View Reviews</a></li>
            <li><a href="@Url.Action("Create","KingdomHeartsReviews")">Create a Review</a></li>
            <li><a href="@Url.Action("Detail","KingdomHeartsReviews")">Kingdom Hearts</a></li>

            <li></li>



        </ul>
    </div>

    <div>
        <h1 style="font-style:italic;">Game Title: @ViewBag.SeriesTitleTwo</h1>
        <h2 style="font-style:italic;">Release Date : @ViewBag.ReleaseYearTwo</h2>

        <br />
        <h5 style="font-style:italic;">Game Description:</h5>
        <div style="font-style:italic;">
            @ViewBag.HtmlDscTwo
            <img src="~/Images/khcom.jpg" class="img-responsive" />
        </div>

        <h5 style="font-style:italic;">Game Developers</h5>
        <div>
            <ul style="font-style:italic;">
                @foreach (var developer in ViewBag.GameDevelopersTwo)
                {
                    <li>@developer</li>
                }
            </ul>
        </div>
        <h5 style="font-style:italic;">Worlds</h5>
        <div>
            <ul style="font-style:italic;">
                @foreach (var world in ViewBag.WorldsTwo)
                {
                    <li>@world</li>
                }
            </ul>

        </div>



    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>